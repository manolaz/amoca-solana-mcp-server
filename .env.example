# RPC endpoint - can be public or private
RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Commitment level for Solana transactions (confirmed, finalized)
COMMITMENT_LEVEL=confirmed

# Helius API Key for enhanced Solana data access
HELIUS_API_KEY=your_helius_api_key_here

# Optional: private key for sending transactions (development only)
# PRIVATE_KEY=your_private_key_in_base58

# Optional: Extra configuration options
# ENABLE_LOGS=true
# MAX_RETRIES=3
